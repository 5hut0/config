{
  "coc.preferences.formatOnSaveFiletypes": ["json", "jsonc"],
  "coc.source.around.enable": false,
  "coc.source.buffer.enable": false,
  "coc.source.emoji.enable": true,
  "coc.source.emoji.triggerCharacters": [":"],
  "coc.source.file.trimSameExts": [],
  "coc.source.omni.filetypes": ["*"],
  "coc.source.syntax.enable": true,
  "codeLens.enable": true,
  "diagnostic.errorSign": "",
  "diagnostic.hintSign": "󿬤",
  "diagnostic.infoSign": "",
  "diagnostic.warningSign": "",
  "git.addedSign.text": "+",
  "git.changeRemovedSign.text": "≃",
  "git.changedSign.text": "~",
  "git.removedSign.text": "_",
  "git.topRemovedSign.text": "‾",
  "json.format.enable": true,
  "list.maxHeight": 30,
  "list.maxPreviewHeight": 30,
  "list.previewHighlightGroup": "Search",
  "list.selectedSignText": "*",
  "list.source.grep.command": "rg",
  "list.source.grep.useLiteral": true,
  "list.source.grep.defaultOptions": ["--interactive"],
  "list.source.grep.args": ["--smart-case", "--vimgrep"],
  "list.source.files.excludePatterns": [
    "**/*.h",
    "**/*.hpp",
    "**/*.ipp",
    "**/*.c",
    "**/*.cpp",
    "**/*.mm",
    "**/*.txt",
    "**/*.sh",
    "**/*.css",
    "**/*.md",
    "**/.git/**",
    "**/*.pem",
    "**/*.msc",
    "**/*.am",
    "**/*.mgw",
    "**/*.icns",
    "**/*.entitlements",
    "**/*.nib",
    "**/*.vsprops",
    "**/*.rc",
    "**/*.qbk",
    "**/*.qml",
    "**/*.qrc",
    "**/*.vcproj",
    "**/*.exp",
    "**/*.vcxproj",
    "**/*.vcxproj.filters",
    "**/*.xcodeproj/**",
    "**/*.xcconfig",
    "**/*.plist",
    "**/*.framework/**",
    "**/*.sln",
    "**/*.wav",
    "**/*.mp3",
    "**/*.eps",
    "**/*.doc",
    "**/*.ttf",
    "**/*.png",
    "**/*.svg",
    "**/*.sketch",
    "**/*.jpeg",
    "**/*.pdf",
    "**/*.jpg"
  ],
  "suggest.autoTrigger": "always",
  "suggest.enablePreview": true,
  "suggest.floatEnable": true,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.timeout": 500,
  "suggest.noselect": false,
  "suggest.minTriggerInputLength": 1,
  "suggest.acceptSuggestionOnCommitCharacter": false,
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "npm",
  "eslint.filetypes": [
    "vue",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "vetur.completion.autoImport": true,
  "vetur.validation.template": true,
  "vetur.experimental.templateInterpolationService": false,
  "vetur.validation.style": true,
  "vetur.validation.script": true,
  "vetur.format.enable": false,
  "tsserver.formatOnType": false,
  "typescript.suggest.autoImports": true,
  "typescript.validate.enable": true,
  "javascript.suggest.autoImports": true,
  "javascript.validate.enable": true,
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        },
        "highlight": { "lsRanges": true },
        "clang": {
          "resourceDir": "/Library/Developer/CommandLineTools/usr/lib/clang/10.0.0",
          "extraArgs": [
            "-isystem",
            "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/include/c++/v1",
            "-I",
            "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/usr/include/"
          ]
        }
      }
    }
  }
}
