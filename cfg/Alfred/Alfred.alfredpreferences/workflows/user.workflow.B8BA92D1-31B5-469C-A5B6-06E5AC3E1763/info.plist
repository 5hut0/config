				<key>applescript</key>
				<string>on alfred_script(q)

tell application "Finder"

	set testvar to q &amp; "~"
	if testvar is equal to "~" then
		set q to "untitled"
	end if


	set loc to insertion location as alias
	set theFile to POSIX path of loc &amp; q

	try
		make new file at loc with properties { name:q }
	on error
		--return theFile
		return "File exists"
	end try

	return theFile

end tell

end alfred_script</string>
